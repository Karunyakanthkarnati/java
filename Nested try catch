class Try {
    public static void main(String[] args) {
        try {int arr[]={10,20,30,40};
            try {
                
                System.out.println(arr[6]);
            } catch (Exception e) {
                System.out.println(e);
            } 
            for(int i=0;i<=4;i++){
           System.out.println(i/0);}
        } catch (Exception e) {
            System.out.println(e);
        }
    }
    
}
